{
  "name": "@cubing/scramble-table",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "module": "./dist/lib/@cubing/scramble-display/index.js",
  "main": "./dist/lib/@cubing/scramble-display/index.js",
  "exports": {
    ".": {
      "import": "./dist/lib/@cubing/scramble-display/index.js"
    }
  },
  "engines": {
    "node": ">=19"
  },
  "files": [
    "dist/lib/@cubing/scramble-display"
  ],
  "scripts": {
    "build": "npm run build-lib && npm run build-types",
    "build-lib": "node script/build-lib.js",
    "build-types": "npx tsc",
    "dev": "node script/dev.js",
    "clean": "rm -rf ./dist",
    "upgrade-cubing": "npm install --save cubing@latest",
    "lint": "npx @biomejs/biome check ./src",
    "format": "npx @biomejs/biome format --write ./src",
    "encrypt-fake-competition": "bun run src/bin/encrypt.ts \"./src/dev/fake-competition/Fake Test Competition.json\" \"./src/dev/fake-competition/Fake Test Competition - Computer Display PDF Passcodes - SECRET.txt\" \"./src/dev/fake-competition/Fake Test Competition.encrypted-scrambles.json\"",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "@fontsource/ubuntu": "^5.0.8",
    "array-buffer-to-hex": "^1.0.0",
    "cubing": "^0.43.8",
    "hex-to-array-buffer": "^2.0.0",
    "libsodium-wrappers": "^0.7.13",
    "pbkdf2-hmac": "^1.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.3.1",
    "@types/libsodium-wrappers": "^0.7.12",
    "@types/node": "^20.8.10",
    "barely-a-dev-server": "^0.6.0",
    "typescript": "^5.2.2"
  },
  "version": "0.0.4"
}
